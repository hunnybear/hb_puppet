{
	"puppet::server": true,
	"puppet::server_external_nodes": "${dir}/puppet_enc.py",
	"puppet::server_foreman": false,
	"puppet::server_reports": "store",
	"hiera::version": 5,
	"hiera::defaults": {
		"datadir": "data",
		"data_hash": "json_data"
	},
	"hiera::backends": ["json", "eyaml"],
	"hiera::hierarchy": [
        {
        	"name":  "Default json file", 
        	"path":  "common.json"
        }
	]
}